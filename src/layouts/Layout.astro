---
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Eid Equity specializes in empowering investors through multifamily investment opportunities. Secure your financial future with our expertise.">
		<meta name="viewport" content="width=device-width" />
		<meta name="keywords" content="Eid Equity, Multifamily Investments, Real Estate Investing, Financial Freedom, Secure Investments">
		<link rel="icon" type="image/svg+xml" href="../../assets/EidEquity.ico" />
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Lato:wght@700&family=Source+Sans+3:wght@200&display=swap" rel="stylesheet">
		<meta name="generator" content={Astro.generator} />

		<meta property="og:title" content="Eid Equity - Multifamily Investment Specialists" />
		<meta property="og:description" content="Eid Equity specializes in empowering investors through multifamily investment opportunities. Secure your financial future with our expertise." />
		<meta property="og:url" content="https://www.eid-equity.vercel.app" />
		<meta property="og:image" content="../../assets/EidEquity.ico" />


		<title>Eid Equity - Multifamily Investment Specialists</title>
	</head>
	<body class="relative">
		<slot />
	</body>
</html>
<style is:global>
	*{
		margin: 0;
		padding: 0;
		box-sizing: border-box;
	}
	html {
		font-family: 'Source Sans 3', sans-serif;
		background-color: #F6F6F6;
	}
	h1{
		font-family: 'Lato', sans-serif;
	}

	a, h3{
		font-weight: 200;
	}
</style>
